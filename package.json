{
  "name": "zhihu-rss-worker",
  "version": "0.1.0",
  "description": "Custom Zhihu RSS feed based on followed users' content and likes",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:init": "wrangler d1 execute zhihu-rss-db --local --file=./schema.sql",
    "db:init:remote": "wrangler d1 execute zhihu-rss-db --remote --file=./schema.sql"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.7.2",
    "wrangler": "^3.93.0"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.1"
  }
}
